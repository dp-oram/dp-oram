{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Target",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/dp-oram/bin/${input:target}",
			"args": [
				"--generateIndices",
				"${input:generateIndices}",
				"--readInputs",
				"${input:readInputs}",
				"--parallel",
				"${input:parallel}",
				"--oramsNumber",
				"${input:oramsNumber}",
				"--useRedis",
				"${input:useRedis}"
			],
			"cwd": "${workspaceFolder}/dp-oram/",
			"preLaunchTask": "build"
		}
	],
	"inputs": [
		{
			"id": "target",
			"type": "pickString",
			"description": "Pick the target file to debug",
			"options": [
				"main"
			],
			"default": "main"
		},
		{
			"id": "generateIndices",
			"type": "pickString",
			"description": "Pick value for --generateIndices",
			"options": [
				"true",
				"false"
			],
			"default": "true"
		},
		{
			"id": "readInputs",
			"type": "pickString",
			"description": "Pick value for --readInputs",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		},
		{
			"id": "parallel",
			"type": "pickString",
			"description": "Pick value for --parallel",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		},
		{
			"id": "oramsNumber",
			"type": "pickString",
			"description": "Pick value for --oramsNumber",
			"options": [
				"1",
				"2",
				"4",
				"8",
				"16"
			],
			"default": "1"
		},
		{
			"id": "useRedis",
			"type": "pickString",
			"description": "Pick value for --useRedis",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		}
	]
}
