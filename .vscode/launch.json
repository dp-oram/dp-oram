{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Test",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/dp-oram/bin/test-${input:test}",
			"args": [],
			"cwd": "${workspaceFolder}/dp-oram/",
			"preLaunchTask": "build"
		},
		{
			"name": "Target",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/dp-oram/bin/${input:target}",
			"args": [
				"--generateIndices",
				"${input:generateIndices}",
				"--readInputs",
				"${input:readInputs}",
				"--parallel",
				"${input:parallel}",
				"--oramsNumber",
				"${input:oramsNumber}",
				"--oramStorage",
				"${input:oramStorage}",
				"--useOrams",
				"${input:useOrams}",
				"--verbosity",
				"TRACE",
				"--count",
				"${input:count}",
				"--virtualRequests",
				"${input:virtualRequests}",
				"--useGamma",
				"${input:useGamma}",
				"--profileStorage",
				"${input:profileStorage}",
			],
			"cwd": "${workspaceFolder}/dp-oram/",
			"preLaunchTask": "build"
		}
	],
	"inputs": [
		{
			"id": "test",
			"type": "pickString",
			"description": "Pick the test file to debug",
			"options": [
				"brc",
				"laplace",
				"mu",
				"padding"
			],
			"default": "laplace"
		},
		{
			"id": "target",
			"type": "pickString",
			"description": "Pick the target file to debug",
			"options": [
				"main",
				"redis-overhead"
			],
			"default": "main"
		},
		{
			"id": "generateIndices",
			"type": "pickString",
			"description": "Pick value for --generateIndices",
			"options": [
				"true",
				"false"
			],
			"default": "true"
		},
		{
			"id": "readInputs",
			"type": "pickString",
			"description": "Pick value for --readInputs",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		},
		{
			"id": "parallel",
			"type": "pickString",
			"description": "Pick value for --parallel",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		},
		{
			"id": "oramsNumber",
			"type": "pickString",
			"description": "Pick value for --oramsNumber",
			"options": [
				"1",
				"2",
				"4",
				"8",
				"16"
			],
			"default": "1"
		},
		{
			"id": "oramStorage",
			"type": "pickString",
			"description": "Pick value for --oramStorage",
			"options": [
				"InMemory",
				"FileSystem",
				"Redis",
				"Aerospike"
			],
			"default": "InMemory"
		},
		{
			"id": "useOrams",
			"type": "pickString",
			"description": "Pick value for --useOrams",
			"options": [
				"true",
				"false"
			],
			"default": "true"
		},
		{
			"id": "count",
			"type": "pickString",
			"description": "Pick value for --count",
			"options": [
				"1000",
				"10000",
				"20000",
				"50000"
			],
			"default": "10000"
		},
		{
			"id": "virtualRequests",
			"type": "pickString",
			"description": "Pick value for --virtualRequests",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		},
		{
			"id": "useGamma",
			"type": "pickString",
			"description": "Pick value for --useGamma",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		},
		{
			"id": "profileStorage",
			"type": "pickString",
			"description": "Pick value for --profileStorage",
			"options": [
				"true",
				"false"
			],
			"default": "false"
		}
	]
}
